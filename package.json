{
  "name": "layout",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "serve": "npm run serve:dynamic",
    "serve:static": "cross-env FILE_MODE=static  vue-cli-service serve",
    "serve:dynamic": "cross-env FILE_MODE=dynamic  vue-cli-service serve",
    "build": "npm run build:dynamic",
    "build:static": "cross-env FILE_MODE=static vue-cli-service build",
    "build:dynamic": "cross-env FILE_MODE=dynamic vue-cli-service build",
    "lint": "vue-cli-service lint",
    "zip": "idm zip"
  },
  "dependencies": {
    "ant-design-vue": "^1.7.6",
    "core-js": "^3.6.5",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@idm-modules/cli": "1.1.2",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.32.8",
    "sass-loader": "^8.0.2",
    "svg-sprite-loader": "^4.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": 0,
      "no-empty": 0,
      "no-debugger": "off",
      "no-undef": 0
    },
    "globals": {
      "IDM": true,
      "$": true,
      "_": true
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
